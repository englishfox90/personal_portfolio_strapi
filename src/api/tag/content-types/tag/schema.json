{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "portfolio_entries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::portfolio-entry.portfolio-entry",
      "inversedBy": "tags"
    },
    "label": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "label"
    },
    "tagType": {
      "type": "enumeration",
      "enum": [
        "program",
        "filter",
        "target",
        "technique",
        "misc"
      ]
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "mappedBy": "tags"
    }
  }
}
